    <DataSet Name="RenderingStaff">
      <Query>
        <DataSourceName>Streamline</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Action">
            <Value>=Parameters!Action.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @Action = 1
	SELECT ID = StaffId,
		StaffName = LastName + ', ' + FirstName
	FROM Staff
	where Active = 'Y'
	and isnull(RecordDeleted, 'N') &lt;&gt; 'Y'
	UNION ALL
	SELECT -1, ' None'
	ORDER BY StaffName
ELSE SELECT ID = -1, StaffName = 'N/A'</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="StaffName">
          <DataField>StaffName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="BillingStaff">
      <Query>
        <DataSourceName>Streamline</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Action">
            <Value>=Parameters!Action.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @Action = 1
	SELECT ID = StaffId,
		StaffName = LastName + ', ' + FirstName
	FROM Staff
	where Active = 'Y'
	and isnull(RecordDeleted, 'N') &lt;&gt; 'Y'
	UNION ALL
	SELECT -1, ' None'
	ORDER BY StaffName
ELSE SELECT ID = -1, StaffName = 'N/A'</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="StaffName">
          <DataField>StaffName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CustomBillingStaffRenderingStaff">
      <Query>
        <DataSourceName>Streamline</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Action">
            <Value>=Parameters!Action.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @Action = 2
SELECT ID = c.CustomBillingStaffRenderingStaffId
	, Label = rs.LastName + ', ' + rs.FirstName + '/' + bs.LastName + ', ' + bs.FirstName + '  ' + ISNULL(CONVERT(varchar,c.FromDate,101), '') + ' - ' + ISNULL(CONVERT(varchar,c.ToDate,101), 'ongoing')
FROM CustomBillingStaffRenderingStaff AS c
JOIN Staff AS rs ON rs.StaffId = c.RenderingStaffId
	AND ISNULL(rs.RecordDeleted,'N') &lt;&gt; 'Y'
JOIN Staff AS bs ON bs.StaffId = c.BillingStaffId
	AND ISNULL(bs.RecordDeleted,'N') &lt;&gt; 'Y'
WHERE ISNULL(c.RecordDeleted,'N') &lt;&gt; 'Y'
UNION ALL
SELECT -1, ' None'
ORDER BY Label
ELSE
SELECT ID = -1, Label = 'N/A'</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Label">
          <DataField>Label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
