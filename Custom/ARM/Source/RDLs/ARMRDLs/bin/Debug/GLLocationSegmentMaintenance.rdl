    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Streamline</DataSourceName>
        <CommandText>select LocationId, LocationCode
from Locations
where isnull(RecordDeleted, 'N') &lt;&gt; 'Y'
union
select null, ' ALL'
order by 2, 1</CommandText>
      </Query>
      <Fields>
