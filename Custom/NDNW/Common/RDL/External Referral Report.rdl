  <DataSets>
    <DataSet Name="GetProviderTypeCode">
      <Query>
        <DataSourceName>Streamline</DataSourceName>
        <CommandText>SELECT NULL AS ProviderTypeId, 'All Types' AS ProviderType
UNION
SELECT GlobalCodeId AS ProviderTypeId, CodeName AS ProviderType
FROM GlobalCodes 
WHERE Category = 'PCPROVIDERTYPE'
AND Active = 'Y'
AND ISNULL(RecordDeleted, 'N') &lt;&gt; 'Y'
ORDER BY ProviderType ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="ProviderTypeId">
          <DataField>ProviderTypeId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProviderType">
          <DataField>ProviderType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetProvidersBasedOnType">
      <Query>
        <DataSourceName>Streamline</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProviderTypeId">
            <Value>=Parameters!ProviderTypeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NULL AS ExternalReferralProviderId, 'All' AS Name
UNION
SELECT ExternalReferralProviderId, Name
FROM ExternalReferralProviders
WHERE Active = 'Y'
AND Type = @ProviderTypeId
AND ISNULL(RecordDeleted, 'N') &lt;&gt; 'Y'
ORDER BY Name ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="ExternalReferralProviderId">
          <DataField>ExternalReferralProviderId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
